{
  "name": "astro-service-ts",
  "version": "1.0.0",
  "description": "Simple Astro Service",
  "main": "app.js",
  "scripts": {
    "build": "npm-run-all timestamp clean tsc",
    "clean": "rimraf dist/*",
    "dockerbuild": "docker build -t fergusonjason/astro-service-ts-express-mongo .",
    "dockerize": "npm-run-all timestamp build dockerbuild",
    "start": "node ./dist/app.js",
    "tsc": "tsc",
    "timestamp": "date > buildtime.txt",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "author": "Jason",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.4.13",
    "app-root-path": "^2.2.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jshint": "^2.10.2",
    "jszip": "^3.2.2",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "papaparse": "^5.1.0",
    "readline": "^1.3.0",
    "stream": "0.0.2",
    "vizier-model": "0.0.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.32",
    "@types/app-root-path": "^1.2.4",
    "@types/chai": "^4.2.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.17",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.5",
    "@types/papaparse": "^5.0.1",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "karma": "^4.4.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-typescript": "^4.1.1",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
